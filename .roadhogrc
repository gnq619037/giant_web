{
  "entry": "src/index.js",
  "publicPath": "/",
  "outputPath": "./dist",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style":true }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style":true }]
      ]
    }
  },
  "proxy": {
    "/server": {
      "target": "http://127.0.0.1:8080/",
      "changeOrigin": true
    }
  }
}
